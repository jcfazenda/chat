{
  "name": "ngx-scrollbar",
  "version": "11.0.0",
  "license": "MIT",
  "homepage": "https://ngx-scrollbar.netlify.com/",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MurhafSousli/ngx-scrollbar"
  },
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-scrollbar/issues"
  },
  "keywords": [
    "scrollbar",
    "smooth-scroll",
    "scroll-reached"
  ],
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@angular/common": ">=15.0.0",
    "@angular/core": ">=15.0.0",
    "@angular/cdk": ">=15.0.0",
    "rxjs": ">=6.0.0"
  },
  "module": "fesm2015/ngx-scrollbar.mjs",
  "es2020": "fesm2020/ngx-scrollbar.mjs",
  "esm2020": "esm2020/ngx-scrollbar.mjs",
  "fesm2020": "fesm2020/ngx-scrollbar.mjs",
  "fesm2015": "fesm2015/ngx-scrollbar.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngx-scrollbar.mjs",
      "es2020": "./fesm2020/ngx-scrollbar.mjs",
      "es2015": "./fesm2015/ngx-scrollbar.mjs",
      "node": "./fesm2015/ngx-scrollbar.mjs",
      "default": "./fesm2020/ngx-scrollbar.mjs"
    },
    "./reached-event": {
      "types": "./reached-event/index.d.ts",
      "esm2020": "./esm2020/reached-event/ngx-scrollbar-reached-event.mjs",
      "es2020": "./fesm2020/ngx-scrollbar-reached-event.mjs",
      "es2015": "./fesm2015/ngx-scrollbar-reached-event.mjs",
      "node": "./fesm2015/ngx-scrollbar-reached-event.mjs",
      "default": "./fesm2020/ngx-scrollbar-reached-event.mjs"
    },
    "./smooth-scroll": {
      "types": "./smooth-scroll/index.d.ts",
      "esm2020": "./esm2020/smooth-scroll/ngx-scrollbar-smooth-scroll.mjs",
      "es2020": "./fesm2020/ngx-scrollbar-smooth-scroll.mjs",
      "es2015": "./fesm2015/ngx-scrollbar-smooth-scroll.mjs",
      "node": "./fesm2015/ngx-scrollbar-smooth-scroll.mjs",
      "default": "./fesm2020/ngx-scrollbar-smooth-scroll.mjs"
    }
  },
  "sideEffects": false
}